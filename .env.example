# Network Configuration
NETWORK=bsc-testnet

# RPC Provider URL
# Examples for different networks:
# Ethereum Mainnet: https://mainnet.infura.io/v3/YOUR_INFURA_KEY
# Goerli Testnet: https://goerli.infura.io/v3/YOUR_INFURA_KEY
# BSC Mainnet: https://bsc-dataseed.binance.org/
# BSC Testnet: https://data-seed-prebsc-1-s1.binance.org:8545/
PROVIDER_URL=https://data-seed-prebsc-1-s1.binance.org:8545/

# Private Keys - IMPORTANT: Never share or commit this file!
# Main wallet private key (without 0x prefix)
PRIVATE_KEY=your_main_wallet_private_key_here

# Script Configuration
# Enable debug mode for additional logging
DEBUG_MODE=true

# Default Fee Tier (100, 500, 3000, 10000)
DEFAULT_FEE_TIER=500

# Gas Settings
GAS_LIMIT=1000000
GAS_PRICE=5000000000  # 5 gwei

# Swap Settings
MIN_AMOUNT=10
MAX_AMOUNT=50
MIN_WAIT_TIME=30
MAX_WAIT_TIME=120
MAX_RETRIES=2

# Number of wallets and swaps
WALLET_COUNT=3
SWAP_COUNT=5